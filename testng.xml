<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="SDET Automation Suite">

	<listeners>
		<listener class-name="com.sdet.framework.listeners.TestListener" />
	</listeners>

	<test name="All Tests">
		<groups>
			<run>
				<include name="${groups}" />
			</run>
		</groups>

		<packages>
			<package name="com.sdet.framework" />
		</packages>
	</test>

</suite>
